{
  "name": "alumni-management-system",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently 'npm run dev:server' 'npm run dev:frontend'",
    "dev:frontend": "vite --port 5173",
    "dev:server": "nodemon -x 'ts-node --project ./tsconfig.node.json server.ts'",
    "prod:server": "npm run build && NODE_ENV=production ts-node --project ./tsconfig.node.json server.ts",
    "seed-db": "ts-node --project ./tsconfig.node.json ./backend/dbSeed.ts",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "better-sqlite3": "^8.3.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "formik": "^2.2.9",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.4",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/node": "^20.1.0",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "nodemon": "^2.0.22",
    "sass": "^1.62.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  }
}
